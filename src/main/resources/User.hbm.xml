<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.alina.common.User" table="users">
		<id column="user_id" name="id" type="java.lang.Long">
			<generator class="increment" />
		</id>
		<property column="first_name" name="firstName" type="java.lang.String" />
		<property column="last_name" name="lastName" type="java.lang.String" />
		<property column="passwords" name="passwords"  type="java.lang.String" />
		<property column="role" name="role" type="java.lang.String" />

	</class>
</hibernate-mapping>